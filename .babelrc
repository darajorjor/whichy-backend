{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    [
      "stage-2"
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "src": "./src",
          "connections": "./src/dao/connections",
          "models": "./src/dao/models",
          "repositories": "./src/dao/repositories"
        }
      },
      "transform-runtime"
    ]
  ],
  "comments": false
}
